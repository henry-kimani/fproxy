{
  "name": "fproxy",
  "private": true,
  "version": "0.0.3",
  "type": "module",
  "main": "dist-electron/main.js",
  "author": "kim henry",
  "scripts": {
    "dev:react": "vite",
    "dev:electron": "electron .",
    "build": "tsc -b && vite build",
    "lint": "eslint .",
    "preview": "vite preview",
    "transpile:electron": "tsc --project src/electron/tsconfig.json -w",
    "dist:mac": "pnpm transpile:electron && pnpm build && electron-builder --mac --arm64",
    "dist:linux": "pnpm transpile:electron && pnpm build && electron-builder --linux --x64",
    "dist:win": "pnpm transpile:electron && pnpm build && electron-builder --win --x64"
  },
  "dependencies": {
    "@hono/node-server": "^1.18.2",
    "dotenv": "^17.2.1",
    "hono": "^4.9.1",
    "react": "^19.1.1",
    "react-dom": "^19.1.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.33.0",
    "@types/node": "^24.2.1",
    "@types/react": "^19.1.10",
    "@types/react-dom": "^19.1.7",
    "@vitejs/plugin-react-swc": "^4.0.0",
    "electron": "^37.2.6",
    "electron-builder": "^26.0.12",
    "eslint": "^9.33.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.20",
    "globals": "^16.3.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.39.1",
    "vite": "^7.1.2"
  }
}
